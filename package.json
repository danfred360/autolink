{
  "name": "autolink",
  "private": true,
  "workspaces": [
    "packages/api",
    "packages/app",
    "packages/auth",
    "packages/database"
  ],
  "scripts": {
    "build": "lerna run build",
    "test": "lerna run test",
    "lint": "run-p lint:*",
    "lint:js": "lerna exec -- eslint --ext .ts,.js,.jsx,.tsx src --cache --cache-location .cache",
    "lint:ts": "lerna exec -- tsc --noEmit",
    "lint:deps": "npm audit --audit-level=high",
    "docker:build": "lerna run docker:build",
    "docker:up": "docker compose up --build"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "lerna": "8.2.1",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.26.0",
    "npm-run-all": "^4.1.5"
  },
  "license": "MIT",
  "volta": {
    "node": "23.9.0",
    "npm": "11.2.0"
  }
}